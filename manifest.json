  {
    "manifest_version": 3,
    "name": "YT Premium",
    "version": "0.0.1",
    "description": "Get YouTube Premium badge + a rewired functional download button!",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*.youtube.com/*"
        ],
        "js": [
          "script.js"
        ]
      }
    ],
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
      "default_popup": "popup.html" 
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
